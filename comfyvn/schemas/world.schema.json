{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ComfyVN World",
  "type": "object",
  "required": [
    "id",
    "title",
    "license",
    "lore"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-z0-9_\\-]+$"
    },
    "title": {
      "type": "string"
    },
    "license": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "source": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "assets": {
      "type": "object"
    },
    "lore": {
      "type": "object"
    },
    "characters": {
      "type": [
        "object",
        "null"
      ]
    },
    "play_examples": {
      "type": [
        "object",
        "null"
      ]
    },
    "compat": {
      "type": [
        "object",
        "null"
      ]
    },
    "notes": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "type": "string"
      }
    }
  }
}